{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["./src/lib/index.ts"],
	"plugin": ["typedoc-plugin-markdown"],
	"out": "docs",
	"readme": "none",
	"disableSources": true,
	"excludeNotDocumented": true,
	"hideGenerator": true,
	"githubPages": false,
	"groupOrder": [
		"Categorical Palettes",
		"Divergent Palettes",
		"Sequential Palettes",
		"Utilities",
		"Functions",
		"References",
		"Modules",
		"Namespaces",
		"Enumerations",
		"Enumeration Members",
		"Classes",
		"Interfaces",
		"Type Aliases",
		"Constructors",
		"Properties",
		"Variables",
		"Accessors",
		"Methods"
	],
	"kindSortOrder": [
		"Function",
		"Reference",
		"Project",
		"Module",
		"Namespace",
		"Enum",
		"EnumMember",
		"Class",
		"Interface",
		"TypeAlias",
		"Constructor",
		"Property",
		"Variable",
		"Accessor",
		"Method",
		"ObjectLiteral",
		"Parameter",
		"TypeParameter",
		"TypeLiteral",
		"CallSignature",
		"ConstructorSignature",
		"IndexSignature",
		"GetSignature",
		"SetSignature"
	]
}
